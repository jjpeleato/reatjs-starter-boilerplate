{
  "name": "reatjs-starter-boilerplate",
  "version": "0.0.0",
  "description": "React JS base repository for any project",
  "keywords": [
    "node",
    "npm",
    "yarn",
    "gulp-v4",
    "stylelint",
    "reactjs",
    "redux",
    "sass",
    "css3"
  ],
  "homepage": "https://www.jjpeleato.com/",
  "author": "@jjpeleato",
  "license": "GPL-2.0",
  "private": true,
  "main": "gulpfile.babel.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjpeleato/reatjs-starter-boilerplate.git"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-snap": "^1.23.0",
    "redux": "^4.0.5",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "autoprefixer": "^10.1.0",
    "browser-sync": "^2.26.13",
    "del": "^6.0.0",
    "dotenv-cli": "^4.0.0",
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-notify": "^3.2.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-stylelint": "^13.0.0",
    "gulp-terser": "^2.0.0",
    "gulp-uglifycss": "^1.1.0",
    "gulp-util": "^3.0.8",
    "husky": "^4.3.6",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:alpha": "env-cmd -f .env.alpha.local react-scripts build",
    "build:beta": "env-cmd -f .env.beta.local react-scripts build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gulp:dev": "gulp",
    "gulp:prod": "env NODE_ENV=production gulp",
    "gulp:validate": "gulp validate",
    "lint": "eslint --ext .jsx,.js src",
    "lint:fix": "eslint --fix --ext .jsx,.js src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run gulp:validate",
      "post-merge": "npm install --save-dev",
      "post-rewrite": "npm install --save-dev"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
